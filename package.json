{
  "name": "premiere",
  "version": "0.2.6",
  "description": "Javascript ORM for consuming Restful APIs",
  "keywords": [
    "restful",
    "api",
    "orm",
    "client",
    "object oriented",
    "oop",
    "caching"
  ],
  "main": "cjs/index.js",
  "module": "es/index.js",
  "umd:main": "umd/index.min.js",
  "files": [
    "cjs",
    "es"
  ],
  "scripts": {
    "test": "jest",
    "test:ci": "npm run build && npm run test:coverage",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "flow": "flow",
    "prettier": "prettier --write \"{scripts,src}/**/*.js\"",
    "precommit": "lint-staged",
    "preversion": "npm test && npm run build"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "casex": "^2.0.0",
    "pluralize": "^7.0.0",
    "query-string": "^6.1.0"
  },
  "license": "MIT",
  "homepage": "https://pedsmoreira.github.io/premiere",
  "repository": "pedsmoreira/premiere",
  "author": "Pedro Moreira",
  "bugs": "https://github.com/pedsmoreira/premiere/issues",
  "devDependencies": {
    "axios-mock-adapter": "^1.15.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^23.4.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "flow-bin": "^0.78.0",
    "flow-remove-types": "^1.2.3",
    "husky": "^0.14.3",
    "jest": "^23.5.0",
    "lint-staged": "^7.2.2",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.14.2",
    "rollup": "^0.64.1"
  },
  "lint-staged": {
    "src/**/*.js": [
      "prettier --write",
      "git add"
    ]
  }
}
